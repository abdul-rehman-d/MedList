{
  "expo": {
    "name": "MedList",
    "slug": "MedList",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E8DEF8"
    },
    "extra": {
      "eas": {
        "projectId": "c42f5e03-3ba6-4909-a18a-2c7b0ccf9a7b"
      }
    },
    "android": {
      "package": "com.abdulrehmand.MedList",
      "permissions": [
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to read CSV files."
      }
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ] 
  }
}
